<UserControl x:Class="MVVM_Event_Bubble_in_multilevel_miewmodel.Chield1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MVVM_Event_Bubble_in_multilevel_miewmodel"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <DataTemplate DataType="{x:Type local:GrandChield1ViewModel}">
            <local:GrandChield1 />
        </DataTemplate>
    </UserControl.Resources>
    <Grid Background="Green">
        <Grid Margin="30">
            <ContentControl Content="{Binding CenterView}"></ContentControl>
        </Grid>

    </Grid>
</UserControl>

<UserControl x:Class="AutoFac_Scope.World.Asia.SouthAsia.SouthAsia"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AutoFac_Scope.World.Asia.SouthAsia"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Margin="5" Background="Azure">
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="35"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="South asia region"></TextBlock>
            <TextBlock Text="    "></TextBlock>
            <TextBlock Text="{Binding Feedback}"></TextBlock>
        </StackPanel>
        <Button Grid.Row="1" Command="{Binding LoadEuropeCommand}" Content="Europe Raise"></Button>
    </Grid>
</UserControl>
